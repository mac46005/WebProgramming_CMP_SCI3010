<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="userModel">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="id" type="xs:positiveInteger"></xs:element>
                <xs:element name="userName" type="xs:string"></xs:element>
                <xs:element name="password" type="xs:string"></xs:element>
                <xs:element name="firstName" type="xs:string"></xs:element>
                <xs:element name="lastName" type="xs:string"></xs:element>
                <xs:element name="address01" type="xs:string"></xs:element>
                <xs:element name="address02" type="xs:string"></xs:element>
                <xs:element name="city" type="xs:string"></xs:element>
                <xs:element name="state"></xs:element>
                <xs:element name="zip" type="xs:string"></xs:element>
                <xs:element name="phoneNumber" type="xs:string"></xs:element>
                <xs:element name="email" type="xs:string"></xs:element>
                <xs:element name="gender" type="xs:string"></xs:element>
                <xs:element name="maritalStatus" type="xs:string"></xs:element>
                <xs:element name="dob" type="xs:string"></xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>